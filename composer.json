{
  "name": "barnicolly/easy-coding-standard-baseline",
  "type": "library",
  "description": "Easy Coding Standard Baseline",
  "keywords": [
    "code-style",
    "code-standard",
    "psr-12",
    "easy-coding-standard",
    "baseline",
    "editorconfig"
  ],
  "license": "MIT",
  "require": {
    "php": "^7.2|^8.0",
    "symfony/var-exporter": "^5.1",
    "symplify/easy-coding-standard": "^9.2"
  },
  "suggest": {
    "symplify/easy-coding-standard-prefixed": "^9.2",
    "slevomat/coding-standard": "^7.0",
    "symfony/var-exporter": "Required for ecs-baseliner"
  },
  "bin": [
    "bin/ecs-baseliner.php"
  ],
  "config": {
    "sort-packages": true
  }
}
